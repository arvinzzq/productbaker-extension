{
  "name": "productbaker-extension",
  "displayName": "ProductBaker",
  "version": "1.0.0",
  "description": "ProductBaker is a powerful browser extension that provides comprehensive website analytics and SEO insights directly in your browser. It is designed for product managers, indie makers, marketers, and SEO specialists who want instant access to actionable data.",
  "author": "ProductBaker Team",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "jsdom": "^26.1.0",
    "plasmo": "^0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "javascript-obfuscator": "^4.1.1",
    "lucide-react": "^0.263.1",
    "postcss": "^8.4.0",
    "prettier": "3.2.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "sidePanel",
      "tabs",
      "activeTab",
      "notifications",
      "scripting"
    ],
    "action": {
      "default_title": "ProductBaker"
    }
  }
}
